<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ar.com.beehive.beehivedemoapp" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GraphExample"
            android:label="@string/title_activity_graph_example" >
        </activity>
        <activity
            android:name=".ButtonExample"
            android:label="@string/title_activity_button_example" >
        </activity>
        <activity
            android:name=".GraphExample2"
            android:label="@string/title_activity_graph_example2" >
        </activity>
    </application>

</manifest>
